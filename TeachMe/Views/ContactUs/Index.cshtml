
@{
    var ContactUsModels = (List<ContactUsModel>)ViewData["_ContactUs"];

}

<h2 style="align-content:center"> Contact Us   </h2>
<div>

    @foreach (var ContactUs in Model)
    {

    <form>
        <div class="form-group">
            <label for="exampleInputName">@ContactUs.User_Name User Name </label>
            <input type="text" class="form-control" id="exampleInputName" placeholder="Name">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">@ContactUs.User_Email Email address</label>
            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Email">
        </div>
        <div class="form-group">
            <label for="exampleInputSubject">@ContactUs.Massege_Subject Subject</label>
            <input type="text" class="form-control" id="exampleInputSubject" placeholder="Subject">
        </div>
        <div class="form-group mb-3">
            <label for="exampleInputMassege">@ContactUs.Massege_Content Massege</label>
            <input type="text" class="form-control" id="exampleInputSubject" placeholder="Massege"  rows="3">
        </div>
     

        <button type="submit" class="btn btn-default">Submit</button>
    </form>
    }
    </div>
